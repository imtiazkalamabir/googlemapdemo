<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/name_section">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="380dp"
        android:weightSum="9"
        android:padding="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        android:background="#ffffff"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="Number of Staff"
            android:paddingLeft="5dp"
            android:textColor="#ffffff"
            android:gravity="left|center_vertical"
            android:background="#696969"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_marginTop="5dp"
            android:weightSum="3"
            android:orientation="horizontal">


            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Total"
                android:gravity="left|center_vertical"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:background="#e9e9e9"
                android:layout_weight="1"/>


            <TextView
                android:id="@+id/total_staff"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Total"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>






        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Physicians"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/physician_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Physicians#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Nurse"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/nurse_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Nurse#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Paramedics"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/paramedics_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Paramedics"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Midwives"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/midwives_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Midwives#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Pharmacists"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/pharmacists_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Pharmacists#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Non-physician clinicians"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/non_phys_clinicians_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Non-physician clinicians#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="3"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Others"
                android:gravity="left|center_vertical"
                android:background="#e9e9e9"
                android:paddingLeft="5dp"
                android:textSize="12sp"
                android:layout_weight="1"/>



            <TextView
                android:id="@+id/others_number"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Others#"
                android:textSize="12sp"
                android:paddingLeft="5dp"
                android:gravity="left|center_vertical"
                android:layout_marginLeft="5dp"
                android:layout_weight="2"/>




        </LinearLayout>





    </LinearLayout>


</RelativeLayout>